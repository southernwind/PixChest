<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ctConverters="using:CommunityToolkit.WinUI.Converters"
	xmlns:converters="using:PixChest.Views.Resources.Converters">
	<ctConverters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
	<converters:ToVisibilityConverter
        x:Key="NullToCollapseConverter"
        Collapse="{x:Null}"
        Visible="*" />
</ResourceDictionary>
